:- register_ros_package(knowrob_objects).
:- register_ros_package(comp_spatial).
:- consult('@IAS_SEMANTIC_MAP_PACKAGE_PATH@/prolog/semantic_map_utils.pl').
:- register_ros_package(suturo_knowledge_foodreasoner).
